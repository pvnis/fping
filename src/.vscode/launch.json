{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug fping",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/fping",
            "args": ["8.8.8.8", "-w", "10", "-c", "1"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "trace": true,
                "traceResponse": true,
                "engineLogging": true
            }
        }
    ]
}